{
    "manifest_version": 3,
    "name": "DailyToDo",
    "version": "1.0",
    "description": "A Chrome extension built with React",
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "side_panel": {
      "default_path": "index.html"
    },
    "action": {
      "default_icon": {
        "16": "assets/icon16.png"
      }
    },
    "permissions": ["sidePanel","history","activeTab"]
  }